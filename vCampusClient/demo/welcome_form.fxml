                    <Insets fx:id="x1" />
                </GridPane.margin>
            </TextField>
            <PasswordField fx:id="loginPassword" prefWidth="200.0" promptText="passsword" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.margin="$x1" GridPane.rowIndex="2" GridPane.valignment="CENTER" GridPane.vgrow="NEVER" />

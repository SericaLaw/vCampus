<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="team.yummy.vCampus.models.entity.BankRecordEntity" table="BankRecord" schema="&quot;.&quot;"
           catalog="&quot;.&quot;">
        <id name="id" column="ID"/>
        <property name="deposit" column="Deposit"/>
        <property name="depositTime" column="DepositTime"/>
        <many-to-one name="bankAccountByBankAccountId" class="team.yummy.vCampus.models.entity.BankAccountEntity">
            <column name="BankAccountID" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>